<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weofapphao">

    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".czm.ui.CzmHomeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:theme="@style/no_title" />
        <activity
            android:name=".czm.ui.Activity.StudObjectAnimator"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:theme="@style/no_title" />

        <activity
            android:name=".czm.ui.Activity.StudSurface"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:theme="@style/no_title" />
        <activity android:name=".wh.HaoMainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".liuzhonghu.LZHActivity" />
        <activity
            android:name=".liuzhonghu.activity.SlidingMenuActivity"
            android:label="@string/title_activity_sliding_menu"
            android:theme="@style/AppTheme2"></activity>

        <!--         Rrtoyewx             -->
        <activity android:name="com.example.weofapphao.xqq.RrtoyewxMainActivity" />
        <activity android:name="com.example.weofapphao.xqq.recycleview.RrtoyewxRecycleViewActivity" />
        <activity android:name=".xqq.explosionfield.RrtoyewxExplosionActivity" />
        <!--         Rrtoyewx             -->
    </application>

</manifest>
